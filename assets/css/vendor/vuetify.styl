@import '~vuetify/src/stylus/settings/_colors'
$theme := {
  primary: $blue.darken-4
  accent: $blue.accent-2
  secondary: $grey.darken-3
  info: $blue.base
  warning: $amber.base
  error: $red.base
  success: $green.base
}

// Import Vuetify styling
@require '~vuetify/src/stylus/main.styl'
